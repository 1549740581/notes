<!-- 基于Eclipse的快捷键，并做了一定修改，修改内容见idea_tutorials.txt-->
<keymap version="1" name="Eclipse copy" parent="Eclipse">
  <action id="CallHierarchy">
    <keyboard-shortcut first-keystroke="shift ctrl h" />
  </action>
  <action id="ChangesView.ShelveSilently" />
  <action id="ClassNameCompletion">
    <keyboard-shortcut first-keystroke="alt slash" />
  </action>
  <action id="Console.Jpa.GenerateSql" />
  <action id="EditorChooseLookupItemCompleteStatement" />
  <action id="EditorCompleteStatement" />
  <action id="EditorStartNewLineBefore">
    <keyboard-shortcut first-keystroke="shift ctrl enter" />
  </action>
  <action id="EditorToggleCase">
    <keyboard-shortcut first-keystroke="shift ctrl x" />
    <keyboard-shortcut first-keystroke="shift ctrl y" />
  </action>
  <action id="HippieCompletion" />
  <action id="MethodHierarchy">
    <keyboard-shortcut first-keystroke="ctrl alt h" />
  </action>
  <action id="ParameterInfo">
    <keyboard-shortcut first-keystroke="ctrl alt slash" />
  </action>
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="shift ctrl n" />
  </action>
  <action id="Run">
    <keyboard-shortcut first-keystroke="alt r" />
  </action>
  <action id="ShelveChanges.UnshelveWithDialog" />
  <action id="ShowUmlDiagram">
    <keyboard-shortcut first-keystroke="shift ctrl u" />
  </action>
  <action id="Unwrap" />
</keymap>